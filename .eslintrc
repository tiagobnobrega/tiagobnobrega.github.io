{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": ["react-app","airbnb-base","prettier"],
  "plugins": ["prettier"],
  "rules":{
    "import/no-unresolved": false,
    "class-methods-use-this":0,
    //    "eslint no-underscore-dangle": 0 globally disable "_" properties
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}
